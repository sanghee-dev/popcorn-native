{
  "expo": {
    "name": "popcorn-native",
    "slug": "popcorn-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#00ff54",
    "backgroundColor": "#beb8b8",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#beb8b8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "co.leeesangheee.popcorn",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "co.leeesangheee.popcorn",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#beb8b8"
      }
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#beb8b8"
    },
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#beb8b8"
    }
  }
}
